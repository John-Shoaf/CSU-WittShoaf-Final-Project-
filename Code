ods noproctitle;
ods graphics / imagemap=on;

proc reg data=WORK.IMPORT alpha=0.05 plots(only)=(diagnostics residuals fitplot 
		observedbypredicted);
	model 'Average with Missing Data'n=Languages /;
	run;
quit;
