SAS Importing Data – Not Written, but used
/* Generated Code (IMPORT) */
/* Source File: Book1.csv */
/* Source Path: /home/u50216647/Enterprise Performance Management */
/* Code generated on: 5/7/23, 4:28 PM */

%web_drop_table(WORK.IMPORT);


FILENAME REFFILE '/home/u50216647/Enterprise Performance Management/Book1.csv';

PROC IMPORT DATAFILE=REFFILE
	DBMS=CSV
	OUT=WORK.IMPORT;
	GETNAMES=YES;
RUN;

PROC CONTENTS DATA=WORK.IMPORT; RUN;


%web_open_table(WORK.IMPORT);

