from prettytable import PrettyTable
mytable = PrettyTable(['Field Name','Data Type','Data Format', 'Field Size','Description', 'Example'])
mytable.add_row([' Year', 'Date', 'YYYY', '4','Year of all other data','2022'])
mytable.add_row([' Country', 'String', 'abcdef', '25','Country Name','Ireland'])
mytable.add_row([' Economic Summary Index Average', 'Integer', '#', '4','Measure of average index against year','1 or 2'])
mytable.add_row([' Languages', 'Integer', '#', '1','Single or multiple official languages','1 or 2'])

print(mytable)
